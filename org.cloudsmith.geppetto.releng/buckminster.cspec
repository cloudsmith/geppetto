        </cs:public>
        <cs:public name="product.zip-gtk.linux.x86_64" actor="ant">
            <cs:actorProperties>
                <cs:property key="buildFile" value="product.ant"/>
                <cs:property key="targets" value="create.product.zip"/>
            </cs:actorProperties>
            <cs:properties>
                <cs:property key="product.arch" value="x86_64"/>
                <cs:property key="product.os" value="linux"/>
                <cs:property key="product.ws" value="gtk"/>
            </cs:properties>
            <cs:prerequisites>
                <cs:attribute name="site.p2" alias="product.repository" component="org.cloudsmith.geppetto.site"/>
                <cs:attribute name="product.configuration.exports" alias="products" component="org.cloudsmith.geppetto.product"/>
                <cs:attribute name="manifest" alias="manifest" component="org.cloudsmith.geppetto.product"/>
